{"remainingRequest":"C:\\src\\vue\\bhhc\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\src\\vue\\bhhc\\src\\components\\EmergencyContact.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\src\\vue\\bhhc\\src\\components\\EmergencyContact.vue","mtime":1576768350613},{"path":"C:\\src\\vue\\bhhc\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\src\\vue\\bhhc\\node_modules\\babel-loader\\lib\\index.js","mtime":499162500000},{"path":"C:\\src\\vue\\bhhc\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\src\\vue\\bhhc\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["EmergencyContact.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6EA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"EmergencyContact.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n<div class=\"\">\r\n    <div class=\"lblTitleRow\">{{typelabel}}</div>\r\n    <div class=\"row topdetail\">\r\n    <div class=\"col\"><label class=\"lbldetail\">Relationship:</label>          \r\n      <b-form-select id=\"inprelationships\" ref=\"inprelationships\" v-model=\"relationshipval\" size=\"sm\">              \r\n        <option v-for=\"relationship in relationshiptypes\" :value=\"fixNull(relationship.ADPVal)\" :key=\"fixNull(relationship.ADPVal)\" >{{fixNull(relationship.TypeVal)}}</option>\r\n      </b-form-select>                         \r\n    </div>  \r\n    <div class=\"col\">\r\n      <label class=\"lbldetail\">Contact Name:</label>\r\n      <b-form-input\r\n          :id=\"emergencycontactnameid\"\r\n          v-model=\"emergencycontactname\"\r\n          :ref=\"emergencycontactnameid\"\r\n          trim\r\n          maxlength=\"40\"\r\n          size=\"sm\"\r\n          :state=\"namestate\"\r\n          aria-describedby=\"input-live-feedback-name\"\r\n      ></b-form-input>        \r\n      <b-form-invalid-feedback id=\"input-live-feedback-name\">\r\n        Must enter a valid name\r\n      </b-form-invalid-feedback> \r\n    </div>    \r\n    <phone \r\n      :entityid=\"entityid\"\r\n      :lmsid=\"lmsid\"      \r\n      :typeid = \"phonetypeid\"\r\n      class=\"col\"\r\n      ptypeof = \"contact\"\r\n      phonetypelbl=\"Contact Phone\"\r\n      phoneareacodeid=\"ContactAreaCode\"\r\n      phoneprefixid=\"ContactPrefix\"\r\n      phonesuffixid=\"ContactSuffix\"\r\n      :phonenumber=\"contactphone\"\r\n      :makeToast=\"makeToast\"\r\n      :runupdate=\"runupdate\"\r\n      :directload=\"phonedirectload\"\r\n      :ft=\"ftprop\"></phone>\r\n    </div>  \r\n    <addressee \r\n      typelabel=\"Emergency Contact Address\"\r\n      streetonelbl=\"Street 1:\"\r\n      streettwolbl=\"Street 2:\"\r\n      streetthreelbl=\"Street 3:\"\r\n      citylbl=\"City\"\r\n      statelbl=\"State\"\r\n      ziplbl=\"Zip\"\r\n      countrylbl=\"Country\"\r\n      streetoneid=\"inpstreetonecontact\"\r\n      streettwoid=\"inpstreettwocontact\"\r\n      streetthreeid=\"inpstreetthreecontact\"\r\n      cityid=\"inpcitycontact\"\r\n      stateid=\"inpstatecontact\"\r\n      countryuid=\"inpcountrycontact\"\r\n      zipid=\"inpzipcontact\"\r\n      :streetone=\"streetonecontact\"\r\n      :streettwo=\"streettwocontact\"\r\n      :streetthree=\"streetthreecontact\"\r\n      :city=\"citycontact\"\r\n      :state=\"statecontact\"\r\n      :zip=\"zipcontact\"\r\n      :country=\"countrycontact\"\r\n      :entityid=\"entityid\"\r\n      :lmsid=\"lmsid\"\r\n      :states=\"states\"\r\n      :countries=\"countries\"           \r\n      typeofaddress=\"contact\"\r\n      :makeToast=\"makeToast\"\r\n      :runupdate=\"runupdate\"\r\n      :ft=\"ftprop\">\r\n    </addressee>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport apiurl from '../config/production';\r\nimport addressee from './Address.vue';\r\nimport phone from './Phone.vue';\r\nvar apival = apiurl();\r\nvar bunyan = require('bunyan');\r\nvar log = bunyan.createLogger({name: \"Enlist_App_EmergencyContactVue\"});\r\nimport {bus} from '../main';\r\n\r\nexport default {\r\n  name: 'emergencycontact',  \r\n  components: { \r\n    addressee,\r\n    phone\r\n  },\r\n  props: { \r\n      entityid:String,\r\n      lmsid:String,\r\n      typelabel:String,\r\n      states:Array,\r\n      countries:Array, \r\n      emergencycontact:Array,\r\n      relationshiptypes:Array,\r\n      emergencycontactnameid:String,\r\n      makeToast: Function,\r\n      fixNull: Function,\r\n      runupdate:Boolean,\r\n      ft:Boolean,\r\n      containsSpecialCharacters:Function,\r\n  },\r\n  data() {\r\n    return {\r\n      relationshipval:'',\r\n      emergencycontactname:'',\r\n      streetonecontact:'',\r\n      streettwocontact:'',\r\n      streetthreecontact:'',\r\n      citycontact:'',\r\n      statecontact:'',\r\n      zipcontact:'',\r\n      countrycontact:'',\r\n      phonetypeid: -99,\r\n      contactphone:'',\r\n      phonedirectload:true,\r\n      runupdatepropval:true,\r\n      ftpropval:true\r\n    };\r\n  },\r\n  mounted:function(){    \r\n    bus.$on('submitsave', this.submit);\r\n    //bus.$on('populatedata', this.bubbleUpData);\r\n    bus.$on('setemergencycontact', this.setEmergencyContact);\r\n  },  \r\n  computed: {\r\n    namestate: function(){\r\n      var isvalid = !!this.emergencycontactname;      \r\n      if (typeof this.containsSpecialCharacters === \"function\") { \r\n        if(isvalid) {\r\n          isvalid = !this.containsSpecialCharacters(this.emergencycontactname);\r\n        }\r\n      }\r\n      return isvalid;\r\n    },\r\n    ftprop:  {      \r\n      get: function() {\r\n        var self = this;\r\n        if(self.ft) {\r\n        self.ftpropval = this.ft;\r\n        }\r\n        return self.ftpropval;\r\n      },\r\n      set: function(newValue) {\r\n        this.ftpropval = newValue;\r\n      }      \r\n    },\r\n    runupdateprop: {      \r\n      get: function() {\r\n        var self = this;\r\n        self.runupdatepropval = this.runupdate;   \r\n        return self.runupdatepropval;\r\n      },\r\n      set: function(newValue) {\r\n        this.runupdatepropval = newValue;\r\n      }      \r\n    },\r\n  },\r\n  methods:{\r\n    setEmergencyContact: function(econt) {\r\n      if(econt){        \r\n        this.ftprop = true;\r\n        this.emergencycontactname = this.fixNull(econt[0].EMER_CONTACT_NAME);\r\n        this.streetonecontact = this.fixNull(econt[0].STREET1);\r\n        this.streettwocontact =  this.fixNull(econt[0].STREET2);\r\n        this.streetthreecontact =  this.fixNull(econt[0].STREET3);\r\n        this.citycontact =  this.fixNull(econt[0].CITY);\r\n        this.statecontact =  this.fixNull(econt[0].STATE);\r\n        this.countrycontact =  this.fixNull(econt[0].COUNTRY);\r\n        this.zipcontact =  this.fixNull(econt[0].ZIP);\r\n        this.relationshipval = this.fixNull(econt[0].RELATIONSHIP);\r\n        this.phonedirectload = true;\r\n        this.contactphone = this.fixNull(econt[0].HOME_PHONE);\r\n        bus.$emit('setphone', this.contactphone, \"contact\");\r\n          this.phonedirectload = false;\r\n        setTimeout(function(){\r\n          this.ftprop = false;\r\n        }, 500);\r\n      }\r\n    },\r\n    submit: function(){   \r\n      var self = this;   \r\n      if(self.runupdateprop == true) {\r\n      var ecstate = !!self.emergencycontactname;\r\n      if(ecstate == true) {\r\n        fetch(apival+'enlistapi/updateemergencycontactdata', {\r\n            method: 'Post',\r\n            credentials: 'include',\r\n            body:JSON.stringify({\r\n              data: {\r\n                'lmsid':self.lmsid,     \r\n                'relationship':self.relationshipval,\r\n                'emergencycontactname':self.emergencycontactname\r\n              }\r\n            }),\r\n            headers:{\r\n              'Accept': 'application/json',\r\n              'Content-Type': 'application/json'\r\n            }\r\n        }).then(function(response) {\r\n          return response.json();\r\n        }).then(function(jsonret) {          \r\n          var resu = jsonret.data;\r\n          if(resu == 'SUCCESS'){\r\n            //self.makeToast(\"emergencycontact Saved\", \"emergencycontact saved successfully.\", \"success\");\r\n            self.runupdateprop = false;         \r\n          }\r\n        }).catch(function(err) {\r\n            log.error('enlistapi/updateemergencycontactdata'+err); \r\n        });        \r\n      } else {\r\n        this.makeToast(\"Emergency Contact\", \"Please correct any errors identified on emergency contact to save the data.\", \"danger\");        \r\n      }\r\n      } \r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n</style>"]}]}