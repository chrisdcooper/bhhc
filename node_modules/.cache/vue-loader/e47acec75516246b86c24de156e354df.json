rstatid\" :ref=\"wfourstatemarstatid\" v-model=\"wfourstatemarstatprop\" size=\"sm\" > \r\n              <option value=\"\" selected>&nbsp;</option>\r\n              <option v-for=\"statemarital in statemaritalstatus\" :value=\"statemarital.ADPValue\" :key=\"statemarital.ADPValue\" >{{statemarital.TypeVal+' - '+statemarital.TypeDesc}}</option>\r\n            </b-form-select>    \r\n          </div>  \r\n          <div class=\"col\"><label class=\"lbldetail\">{{wfourstatetaxcodeslbl}}</label>                \r\n            <b-form-select :id=\"wfourstatetaxcodesid\" :ref=\"wfourstatetaxcodesid\" v-model=\"wfourstatetaxcodesprop\" size=\"sm\" > \r\n              <option value=\"\" selected>&nbsp;</option>\r\n              <option v-for=\"statetaxcode in statetaxcodes\" :value=\"statetaxcode.CodeVal\" :key=\"statetaxcode.CodeVal\" >({{statetaxcode.CodeVal}})&nbsp;{{statetaxcode.State}}</option>\r\n            </b-form-select>    \r\n          </div> \r\n          <div class=\"col\"><label class=\"lbldetail\">{{wfourstatetaxblocklbl}}</label>             \r\n            <b-form-checkbox :id=\"wfourstatetaxblockid\" :ref=\"wfourstatetaxblockid\" v-model=\"wfourstatetaxblockprop\" name=\"check-button\" switch>\r\n              {{wfourstatetaxblocklbl.replace(\":\",\"\")}}\r\n            </b-form-checkbox>\r\n          </div>\r\n        </div>  \r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport apiurl from '../config/production';\r\nvar apival = apiurl();\r\nvar bunyan = require('bunyan');\r\nvar log = bunyan.createLogger({name: \"Enlist_App_WFourVue\"});\r\nimport {bus} from '../main';\r\n\r\nexport default {\r\n  name: 'wfour',  \r\n  components: { \r\n  },\r\n  props: { \r\n      entityid:String,\r\n      lmsid:String,\r\n\r\n      wfourfedtypelbl:String,\r\n      wfourfedexemptptslbl:String,\r\n      wfourfedexemptptsid:String,\r\n\r\n      wfourfedaddtllbl:String,\r\n      wfourfedaddtlid:String,\r\n\r\n      wfourfedsuilbl:String,\r\n      wfourfedsuiid:String,\r\n      \r\n      wfourfedmarstatlbl:String,\r\n      wfourfedmarstatid:String,\r\n\r\n      wfourfedtaxblocklbl:String,\r\n      wfourfedtaxblockid:String,\r\n\r\n      wfourstatetypelbl:String,\r\n      wfourstateexemptptslbl:String,\r\n      wfourstateexemptptsid:String,\r\n\r\n      wfourstateaddtllbl:String,\r\n      wfourstateaddtlid:String,\r\n      \r\n      wfourstatemarstatlbl:String,\r\n      wfourstatemarstatid:String,\r\n\r\n      wfourstatetaxblocklbl:String,\r\n      wfourstatetaxblockid:String,\r\n\r\n      wfourstatetaxcodeslbl:String,\r\n      wfourstatetaxcodesid:String,\r\n\r\n      statetaxcodes:Array, \r\n      fedmaritalstatus:Array, \r\n      statemaritalstatus:Array,\r\n      suicodes:Array,\r\n      makeToast: Function,      \r\n      camelize: Function,\r\n      runupdate:Boolean,\r\n      fixNull: Function,\r\n      ft:Boolean,\r\n      containsSpecialCharacters:Function,\r\n  },\r\n  data() {\r\n    return {\r\n      wfourfedexemptptsprop:'',\r\n      wfourfedaddtlprop:'',\r\n      wfourfedmarstatprop:'',\r\n      wfourfedsuiprop:'',\r\n      wfourfedtaxblockprop:false,\r\n      wfourstateexemptptsprop:'',\r\n      wfourstateaddtlprop:'',\r\n      wfourstatemarstatprop:'',\r\n      wfourstatetaxblockprop:false,\r\n      wfourstatetaxcodesprop:'',\r\n      runupdatepropval:true\r\n    };\r\n  },\r\n  mounted:function(){    \r\n    bus.$on('submitsave', this.submit);\r\n    bus.$on('setwfourtaxdata', this.setWFourTaxData);\r\n  },  \r\n  computed: {\r\n    wfourstate:function(){\r\n      return this.wfourfedexemptptsstate && this.wfourfedadditionalstate && this.wfourstateexemptptsstate && this.wfourstateadditionalstate;\r\n    },\r\n    wfourfedexemptptsstate:function(){\r\n      var isvalid = !!this.wfourfedexemptptsprop && !isNaN(this.wfourfedexemptptsprop);            \r\n      if (typeof this.containsSpecialCharacters === \"function\") { \r\n        if(isvalid) {\r\n          isvalid = !this.containsSpecialCharacters(this.wfourfedexemptptsprop);\r\n        }\r\n      }\r\n      return isvalid;\r\n    },\r\n    wfourfedadditionalstate:function(){\r\n      var isvalid = !!this.wfourfedaddtlprop && !isNaN(this.wfourfedaddtlprop);      \r\n      if (typeof this.containsSpecialCharacters === \"function\") { \r\n        if(isvalid) {\r\n          isvalid = !this.containsSpecialCharacters(this.wfourfedaddtlprop);\r\n        }\r\n      }\r\n      return isvalid;\r\n    },\r\n    wfourstateexemptptsstate:function(){\r\n      var isvalid = !!this.wfourstateexemptptsprop && !isNaN(this.wfourstateexemptptsprop);      \r\n      if (typeof this.containsSpecialCharacters === \"function\") { \r\n        if(isvalid) {\r\n          isvalid = !this.containsSpecialCharacters(this.wfourstateexemptptsprop);\r\n        }\r\n      }\r\n      return isvalid;\r\n    },\r\n    wfourstateadditionalstate:function(){\r\n      var isvalid = !!this.wfourstateaddtlprop && !isNaN(this.wfourstateaddtlprop);      \r\n      if (typeof this.containsSpecialCharacters === \"function\") { \r\n        if(isvalid) {\r\n          isvalid = !this.containsSpecialCharacters(this.wfourstateaddtlprop);\r\n        }\r\n      }\r\n      return isvalid;\r\n    },\r\n    runupdateprop: {      \r\n      get: function() {\r\n        var self = this;  \r\n        self.runupdatepropval = this.runupdate;   \r\n        return self.runupdatepropval;\r\n      },\r\n      set: function(newValue) {\r\n        this.runupdatepropval = newValue;\r\n      }      \r\n    },\r\n  },\r\n  methods:{\r\n    setWFourTaxData: function(payload){\r\n      if(payload){\r\n        if(payload.length > 0){\r\n          this.wfourfedexemptptsprop=this.fixNull(payload[0].FED_ALLOWANCES);\r\n          this.wfourfedaddtlprop=this.fixNull(payload[0].FEDERAL_ADDL_AMT);\r\n          this.wfourfedmarstatprop=this.fixNull(payload[0].FEDERAL_MAR_STATUS);\r\n          var fblock = this.fixNull(payload[0].FEDERAL_TAX_BLOCK);\r\n          this.wfourfedtaxblockprop=(fblock!=='' && fblock!=='0'?true:false);\r\n          this.wfourfedsuiprop=this.fixNull(payload[0].SUI_TAX_CD);\r\n          this.wfourstateexemptptsprop=payload[0].EXEMPTIONS;\r\n          this.wfourstateaddtlprop=this.fixNull(payload[0].ADDL_TAX_AMT);\r\n          this.wfourstatemarstatprop=this.fixNull(payload[0].MARITAL_STATUS);\r\n          var sblock = this.fixNull(payload.TAX_BLOCK);\r\n          this.wfourstatetaxblockprop=(sblock!=='' && sblock!=='0'?true:false);\r\n          this.wfourstatetaxcodesprop=this.fixNull(payload[0].STATE_TAX_CD);\r\n        }\r\n      }\r\n    },\r\n    submit: function(){    \r\n      if(this.runupdateprop == true){\r\n        if(this.wfourstate) {             \r\n          var self = this;   \r\n          var jsonvals = JSON.stringify({\r\n                data: {\r\n                  'lmsid':self.lmsid,     \r\n                  'fedexemptpts':self.wfourfedexemptptsprop,     \r\n                  'fedaddtl':self.wfourfedaddtlprop,     \r\n                  'fedmarstat':self.wfourfedmarstatprop,     \r\n                  'fedtaxblock':self.wfourfedtaxblockprop==true?'1':'',     \r\n                  'fedsui':self.wfourfedsuiprop,     \r\n                  'stateexemptpts':self.wfourstateexemptptsprop,     \r\n                  'stateaddtl':self.wfourstateaddtlprop,     \r\n                  'statemarstat':self.wfourstatemarstatprop,     \r\n                  'statetaxblock':self.wfourstatetaxblockprop==true?'1':'',     \r\n                  'statetaxcode':self.wfourstatetaxcodesprop\r\n                }\r\n              });\r\n          fetch(apival+'enlistapi/updatewfourdata', {\r\n              method: 'Post',\r\n              credentials: 'include',\r\n              body:jsonvals,\r\n              headers:{\r\n                'Accept': 'application/json',\r\n                'Content-Type': 'application/json'\r\n              }\r\n          }).then(function(response) {\r\n            return response.json();\r\n          }).then(function(jsonret) {          \r\n            var resu = jsonret.data;\r\n            if(resu == 'SUCCESS'){\r\n              //self.makeToast(\"wfour Saved\", \"wfour saved successfully.\", \"success\");\r\n              self.runupdateprop = false;         \r\n              bus.$emit('UpdatewfourRowData',  jsonvals);\r\n            }\r\n          }).catch(function(err) {\r\n              log.error('enlistapi/updatewfourdata'+err); \r\n          });                  \r\n        } else {\r\n          this.makeToast('W4 Tax Data', 'Please correct any errors identified on the W4 Tax Data to save the data.', \"danger\");\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.wfourboxseperator{float: left;}\r\n.wfourboxsmall{float: left;max-width: 100px}\r\n.wfourboxlarge{float: left;max-width: 250px}\r\n</style>"]}]}